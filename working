module.exports.run = async ({  event, api, handleReply, Currencies, getText }) => {
    const { threadID, messageID, senderID } = event;
    const cooldown = global.configModule[this.config.name].cooldownTime;
    let data = (await Currencies.getData(senderID)).data || {};
    //cooldownTime cho má»—i láº§n nháº­n 
    if (typeof data !== "undefined" && cooldown - (Date.now() - data.work2Time) > 0) {

        var time = cooldown - (Date.now() - data.work2Time),
            minutes = Math.floor(time / 60000),
            seconds = ((time % 60000) / 1000).toFixed(0); 
        return api.sendMessage(getText("cooldown", minutes, (seconds < 10 ? "0" + seconds : seconds)), event.threadID, event.messageID);
    }
    else {    
        var msg = {
            body: "=== KIáº¾M TIá»€N Má»–I NGÃ€Y ===" +
                "\n\n1. Khu cÃ´ng nghiá»‡p." +
                "\n2. Khu dá»‹ch vá»¥." +
                "\n3. Khu má» dáº§u." +
                "\n4. Khai thÃ¡c quáº·ng." +
                "\n5. Äá»©ng Ä‘Æ°á»ng." +
                "\n6. Tráº§n Duy HÆ°ng." +
                "\n7. Pháº¡m NgÅ© LÃ£o." +
                "\n8. Nguyá»…n Huy Tá»±." +
                "\n\nâš¡ï¸HÃ£y reply tin nháº¯n vÃ  chá»n theo sá»‘.",
                attachment: fs.createReadStream(__dirname + `/cache/working.png`)}
                return api.sendMessage(msg,e.threadID,  (error, info) => {
                data.work2Time = Date.now();
        global.client.handleReply.push({
            type: "choosee",
            name: this.config.name,
            author: e.senderID,
            messageID: info.messageID
          })  
        })
    }
}
